{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "apphosting": {
    "backend": {
      "runConfig": {
        "maxInstances": 1
      }
    }
  },
  "emulators": {
