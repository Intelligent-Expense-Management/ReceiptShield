{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "apphosting": [
    {
      "backendId": "receiptshield-backend",
      "primaryRegion": "us-central1",
      "runConfig": {
        "maxInstances": 1
      },
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/.next/**",
        "**/out/**",
        "**/ml/**",
        "**/docs/**",
        "**/scripts/**",
        "**/.github/**",
        "**/*.md",
        "**/deploy.sh",
        "**/env.*",
        "**/venv/**",
        "**/receipts/**",
        "**/*.log",
        "**/firebase-debug.log"
      ]
    }
  ],
  "emulators": {
    "auth": {
      "port": 9099
    },
    "firestore": {
      "port": 8080
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    }
  }
}